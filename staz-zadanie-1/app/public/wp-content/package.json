{
  "name": "developress",
  "title": "developress project",
  "version": "1.0.0",
  "main": "src/common.js",
  "scripts": {
    "lint:php": "composer run-script lint",
    "lint:php-fix": "composer run-script lint:fix",
    "lint:scss": "npx stylelint './themes/theme/**/*.scss' './plugins/plugin/**/*.scss'",
    "lint:scss-fix": "npx stylelint './themes/theme/**/*.scss' './plugins/plugin/**/*.scss' --fix",
    "lint:js": "npx eslint './themes/theme/**/*.js' './plugins/plugin/**/*.js'",
    "lint:js-fix": "npx eslint './themes/theme/**/*.js' './plugins/plugin/**/*.js' --fix",
    "dev": "webpack --mode=development --watch",
    "start": "npm run dev",
    "build": "webpack --mode=production",
    "bundle:theme": "npm run build && gulp bundleTheme",
    "bundle:vendor": "gulp bundleVendor",
    "bundle:plugin": "gulp bundlePlugin",
    "bundle:everything": "npm run bundle:plugin && npm run bundle:theme && npm run bundle:vendor"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "@wordpress/block-editor": "^9.7.0",
    "@wordpress/blocks": "^11.14.0",
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/components": "^19.17.0",
    "@wordpress/editor": "^12.14.0",
    "@wordpress/element": "^4.13.0",
    "@wordpress/eslint-plugin": "^9.3.0",
    "@wordpress/hooks": "^3.22.0",
    "@wordpress/i18n": "^4.15.0",
    "@wordpress/prettier-config": "^1.4.0",
    "autoprefixer": "^10.4.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "browserslist": "^4.21.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.30.1",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "stylelint-webpack-plugin": "^3.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "eslint-config-wordpress": "^2.0.0",
    "gsap": "file:private-registry/gsap-bonus.tgz"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
